//set up linewidth
strokeWeight(0);

//Set up Red Square
size(1000, 1000);
fill(255, 0, 0);
rect(0,0,500,500);

//set up yellow circle
fill(255, 255, 0);
ellipse(250, 300, 400, 400);

//set up cyan triangle
fill(0, 255, 255);
triangle(250, 75, 450, 450, 50, 450);

//set up grey iris
fill(100, 100, 100);
ellipse(250, 275, 200, 100);

//set up black pupil
fill(0, 0, 0);
ellipse(250, 275, 75, 75);